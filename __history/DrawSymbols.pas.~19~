unit DrawSymbols;

interface

uses
  System.Types, Vcl.Graphics;

procedure DrawProcessSymbol(canva: TCanvas; x, y, width, height: integer);
procedure InitDraw();

var
  boundColor: TColor;
  fillColor: TColor;

implementation

procedure DrawProcessSymbol(canva: TCanvas; x, y, width, height: integer);
begin

  canva.Pen.Color := boundColor;
  canva.Brush.Color := fillColor;

  canva.Rectangle(x, y, width, height);

end;

procedure SetCanvaAttributes();
begin

end;

procedure InitDraw();
begin
  boundColor := clBlack;
  fillColor := clWhite;
end;

end.
